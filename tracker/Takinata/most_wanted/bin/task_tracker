#!/home/snitch/PycharmProjects/TakinataMain/most_wanted/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'console-api==0.1','console_scripts','task_tracker'
__requires__ = 'console-api==0.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('console-api==0.1', 'console_scripts', 'task_tracker')()
    )
